{
  "name": "backend-v2",
  "version": "2.0.0",
  "description": "Vets 4 Warriors Counselor Laptop Resource Version 2 Backend",
  "main": "server.js",
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "start": "node server.js",
    "start-prod": "cross-env NODE_ENV=production node server.js",
    "start-dev": "cross-env NODE_ENV=development node server.js",
    "test-mocha": "cross-env NODE_ENV=testing mocha --reporter=nyan ./test/**/*.spec.js",
    "test-dredd": "cross-env NODE_ENV=testing dredd",
    "test": "yarn run test-mocha && yarn run test-dredd",
    "lint": "eslint --fix --ext .js lib/**/* migrations/**/*",
    "clean": "rm -rf logs",
    "servedocs": "swagger serve swagger.yaml",
    "migrate": "migrate --template-file ./migrations/template.js",
    "migrate-debug": "node $NODE_DEBUG_OPTION ./node_modules/.bin/migrate --template-file ./migrations/template.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vets4Warriors/backend-v2.git"
  },
  "author": "austince <austin.cawley@gmail.com?",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vets4Warriors/backend-v2/issues"
  },
  "homepage": "https://github.com/fbueti/stevens-scheduler-2.0#readme",
  "dependencies": {
    "auth0": "^2.6.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "dredd": "3.3",
    "express": "^4.15.2",
    "express-jwt": "^5.3.0",
    "express-winston": "^2.3.0",
    "http-errors": "^1.6.1",
    "http-status-codes": "^1.1.6",
    "jwks-rsa": "^1.1.1",
    "migrate": "^0.2.3",
    "mongoose": "^4.9.3",
    "mongoose-geojson-schema": "^2.1.2",
    "mongoose-paginate": "^5.0.3",
    "mongoose-schema-extend": "^0.2.2",
    "mongoose-type-email": "^1.0.5",
    "mongoose-type-url": "^1.0.2",
    "mongoose-version": "^1.1.0",
    "passport-google-oauth20": "^1.0.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "require-directory": "^2.1.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-react": "^6.10.3",
    "eslint-to-editorconfig": "^1.2.0",
    "mocha": "^3.2.0",
    "swagger-express": "^1.0.5"
  }
}
